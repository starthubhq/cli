{
  "name": "create-do-project-and-droplet",
  "version": "0.1.1",
  "description": "Create a DigitalOcean project, then create a droplet inside that project.",
  "inputs": [
    {
      "name": "name",
      "type": "string"
    },
    {
      "name": "access_token",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "name": "project_id",
      "type": "string"
    }
  ],
  "steps": [
    {
      "id": "create_do_project_docker",
      "uses": "ghcr.io/starthubhq/create-do-project-docker:0.0.3",
      "with": {}
    }
  ],
  "wires": [
    {
      "from": {
        "source": "inputs",
        "key": "name"
      },
      "to": {
        "step": "create_do_project_docker",
        "input": "do_project_name"
      }
    },
    {
      "from": {
        "source": "inputs",
        "key": "access_token"
      },
      "to": {
        "step": "create_do_project_docker",
        "input": "do_access_token"
      }
    }
  ],
  "export": {
    "project_id": {
      "from": {
        "step": "create_do_project_docker",
        "output": "project_id"
      }
    }
  }
}