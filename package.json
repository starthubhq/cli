{
  "name": "@starthub/cli",
  "version": "0.2.18",
  "bin": {
    "starthub": "npm/dist/launch.js"
  },
  "scripts": {
    "preversion": "git diff --quiet && git diff --cached --quiet || (echo '‚ùå Working tree not clean. Commit or stash first.' && exit 1)",
